<?xml version="1.0" encoding="UTF-8"?>
<module type="JAVA_MODULE" version="4">
  <component name="NewModuleRootManager" inherit-compiler-output="true">
    <exclude-output />
    <content url="file://$MODULE_DIR$">
      <sourceFolder url="file://$MODULE_DIR$/src" isTestSource="false" />
      <sourceFolder url="file://$MODULE_DIR$/src/test" isTestSource="true" />
      <sourceFolder url="file://$MODULE_DIR$/src/main/resources" isTestSource="false" />
    </content>

    <!-- Dependencias del módulo -->
    <orderEntry type="inheritedJdk" />
    <orderEntry type="sourceFolder" forTests="false" />

    <!-- Agregar dependencias externas, como bibliotecas -->
    <orderEntry type="library" name="JUnit4" level="project" />
    <orderEntry type="library" name="Maven Dependencies" level="project" />

    <!-- Dependencias de otros módulos -->
    <orderEntry type="module" module-name="otroModulo" />

    <!-- Configuración de compilador -->
    <compiler>
      <output url="file://$MODULE_DIR$/out" />
      <output testSource="true" url="file://$MODULE_DIR$/out/test" />
    </compiler>
  </component>
</module>
